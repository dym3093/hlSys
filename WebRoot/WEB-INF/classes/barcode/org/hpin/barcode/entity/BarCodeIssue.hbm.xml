<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.hpin.barcode.entity.BarCodeIssue" table="ERP_BAR_CODE_ISSUE"  dynamic-insert="true" dynamic-update="true">
        <id name="id" type="java.lang.String">
            <column name="ID"/>
            <generator class="uuid" />
        </id>     
             
         <property name="companyId" type="java.lang.String">
            <column name="COMPANY_ID"/>
        </property>
          <property name="companyName" type="java.lang.String">
            <column name="COMPANY_NAME"/>
        </property>       
          <property name="contacts" type="java.lang.String">
            <column name="CONTACTS"/>
        </property>
          <property name="contactsTel" type="java.lang.String">
            <column name="CONTACTS_TEL"/>
        </property>
         <property name="sendDate" type="java.lang.String">
            <column name="SEND_DATE"/>
        </property>
          <property name="receiveDate" type="java.lang.String">
            <column name="RECEIVE_DATE"/>
        </property>
         <property name="postcode" type="java.lang.String">
            <column name="POSTCODE"/>
        </property>
          <property name="address" type="java.lang.String">
            <column name="ADDRESS"/>
        </property>        
          <property name="expressNo" type="java.lang.String">
            <column name="EXPRESS_NO"/>
        </property>
         <property name="barcodeCount" type="java.lang.Integer">
            <column name="BARCODE_COUNT"/>
        </property>
          <property name="remark" type="java.lang.String">
            <column name="REMARK"/>
        </property>                             
         <property name="status" type="java.lang.Integer">
            <column name="STATUS"/>
        </property>
         
        <property name="createUserId" type="java.lang.String">
            <column name="CREATE_USER_ID"/>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME"/>
        </property>  
        <property name="isDeleted" type="java.lang.Integer">
            <column name="IS_DELETED"/>
        </property>
          
    </class>
</hibernate-mapping>
