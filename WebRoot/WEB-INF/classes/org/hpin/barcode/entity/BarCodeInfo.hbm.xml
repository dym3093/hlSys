<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.hpin.barcode.entity.BarCodeInfo" table="ERP_BAR_CODE_DETAIL"  dynamic-insert="true" dynamic-update="true">
        <id name="id" type="java.lang.String">
            <column name="ID"/>
            <generator class="uuid" />
        </id>
        <property name="batNO" type="java.lang.String">
            <column name="BAT_NO"/>
        </property>
         <property name="barCode" type="java.lang.String">
            <column name="BAR_CODE"/>
        </property>
        <property name="isEffective" type="java.lang.Integer">
            <column name="IS_EFFECTIVE"/>
        </property>     
    </class>
</hibernate-mapping>
