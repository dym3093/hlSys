<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.hpin.barcode.entity.BarCodeBat" table="ERP_BAR_CODE_BAT"  dynamic-insert="true" dynamic-update="true">
        <id name="id" type="java.lang.String">
            <column name="ID"/>
            <generator class="uuid" />
        </id>
        <property name="batNO" type="java.lang.String">
            <column name="BAT_NO"/>
        </property>
         <property name="remark" type="java.lang.String">
            <column name="REMARK"/>
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="STATUS"/>
        </property>
        <property name="batCount" type="java.lang.Integer">
            <column name="BAT_COUNT"/>
        </property>
         <property name="batActualCount" type="java.lang.Integer">
            <column name="BAT_ACTUAL_COUNT"/>
        </property>
        
         <property name="sendDate" type="java.lang.String">
            <column name="SEND_DATE"/>
        </property>
         <property name="completeDate" type="java.lang.String">
            <column name="COMPLETE_DATE"/>
        </property>
         <property name="remark1" type="java.lang.String">
            <column name="REMARK1"/>
        </property>
        
        
        <property name="createUserId" type="java.lang.String">
            <column name="CREATE_USER_ID"/>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME"/>
        </property>
        <property name="createDeptId" type="java.lang.String">
            <column name="CREATE_DEPT_ID"/>
        </property>
    </class>
</hibernate-mapping>
