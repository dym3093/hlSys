<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.hpin.warehouse.entity.ErpApplication" table="ERP_APPLICATION">
        <id name="id" type="java.lang.String">
			<column name="ID" />
			<generator class="uuid" />
		</id>
        
        <property name="applicationNo" type="java.lang.String">
            <column name="APPLICATION_NO"/>
        </property>
        <property name="bannyCompanyId" type="java.lang.String">
            <column name="BANNY_COMPANY_ID"/>
        </property>
        <property name="projectCode" type="java.lang.String">
            <column name="PROJECT_CODE"/>
        </property>
        <property name="projectName" type="java.lang.String">
            <column name="PROJECT_NAME"/>
        </property>
        <property name="projectOwner" type="java.lang.String">
            <column name="PROJECT_OWNER"/>
        </property>
        <property name="receiveName" type="java.lang.String">
            <column name="RECEIVE_NAME"/>
        </property>
        <property name="receiveTel" type="java.lang.String">
            <column name="RECEIVE_TEL"/>
        </property>
        <property name="address" type="java.lang.String">
            <column name="ADDRESS"/>
        </property>
        <property name="hopeDate" type="java.util.Date">
            <column name="HOPE_DATE"/>
        </property>
        <property name="requirement" type="java.lang.String">
            <column name="REQUIREMENT"/>
        </property>
        <property name="status" type="java.lang.String">
            <column name="STATUS"/>
        </property>
        <property name="attachmentPath" type="java.lang.String">
            <column name="ATTACHMENT_PATH"/>
        </property>
        <property name="linkName" type="java.lang.String">
            <column name="LINK_NAME"/>
        </property>
        <property name="linkTel" type="java.lang.String">
            <column name="LINK_TEL"/>
        </property>
        <property name="dealUserId" type="java.lang.String">
            <column name="DEAL_USER_ID"/>
        </property>
        
        <!-- 默认字段 -->
        <property name="createUserId" type="java.lang.String">
            <column name="CREATE_USER_ID"/>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME"/>
        </property>
        <property name="updateUserId" type="java.lang.String">
            <column name="UPDATE_USER_ID"/>
        </property>
        <property name="updateTime" type="java.util.Date">
            <column name="UPDATE_USER_TIME" />
        </property>
        <property name="isDeleted" type="java.lang.Integer">
            <column name="IS_DELETED" precision="4" scale="0" />
        </property>
        <property name="isMark" type="java.lang.String">
            <column name="IS_MARK"/>
        </property>
    </class>
</hibernate-mapping>
