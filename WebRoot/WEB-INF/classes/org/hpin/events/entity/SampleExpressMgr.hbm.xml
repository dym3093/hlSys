<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.hpin.events.entity.SampleExpressMgr" table="ERP_SAMPLE_EXPRESS_MGR">
		<id name="id" type="java.lang.String">
			<column name="ID" />
		</id>
		<property name="expressNum" type="java.lang.String">
			<column name="EXPRESS_NUM"/>
		</property>
		<property name="expressCompanyId" type="java.lang.String">
			<column name="EXPRESS_COMPANY_ID"/>
		</property>
		<property name="receiveSendType" type="java.lang.Integer">
			<column name="RECEIVE_SEND_TYPE"/>
		</property>
		<property name="receiveSendDate" type="java.util.Date">
			<column name="RECEIVE_SEND_DATE"/>
		</property>
		<property name="isbill" type="java.lang.Integer">
			<column name="ISBILL"/>
		</property>
		<property name="weight" type="java.lang.String">
			<column name="WEIGHT"/>
		</property>
		<property name="totalCost" type="java.math.BigDecimal">
			<column name="TOTAL_COST"/>
		</property>
		<property name="expressContent" type="java.lang.Integer">
			<column name="EXPRESS_CONTENT"/>
		</property>
		<property name="createUserId" type="java.lang.String">
			<column name="CREATE_USER_ID"/>
		</property>
		<property name="createUserName" type="java.lang.String">
			<column name="CREATE_USER_NAME"/>
		</property>
		<property name="isdeleted" type="java.lang.Integer">
			<column name="ISDELETED"/>
		</property>
		<property name="createDate" type="java.util.Date">
			<column name="CREATE_DATE"/>
		</property>
	</class>
</hibernate-mapping>
