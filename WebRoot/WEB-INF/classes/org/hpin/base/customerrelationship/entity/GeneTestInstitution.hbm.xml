<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.hpin.base.customerrelationship.entity.GeneTestInstitution" table="GENE_TEST_INSTITUTION ">
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="uuid" />
        </id>
        <property name="geneTestInstitutionName" type="java.lang.String">
            <column name="TEST_INSTITUTION_NAME"  />
        </property>
    </class>
</hibernate-mapping>
