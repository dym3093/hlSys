<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.hpin.base.customerrelationship.entity.ProjectType" table="T_PROJECT_TYPE"  dynamic-update="true" dynamic-insert="true">
        <id name="id" type="java.lang.String">
            <column name="ID" length="32" />
            <generator class="uuid" />
        </id>
        <property name="projectTypeName" type="java.lang.String">
            <column name="PROJECT_TYPE_NAME" length="200" />
        </property>
        
        <property name="remark" type="java.lang.String">
            <column name="REMARK" length="200" />
        </property>
         <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME" precision="7"/>
        </property>
        <property name="createUserId" type="java.lang.String">
            <column name="CREATE_USER_ID" length="32" />
        </property>
        <property name="isDeleted" type="java.lang.String">
            <column name="IS_DELETED" length="4" />
        </property>
        <property name="deleteTime" type="java.util.Date">
            <column name="DELETE_TIME" length="7" />
        </property>
        <property name="deleteUserId" type="java.lang.String">
            <column name="DELETE_USER_ID" length="32" />
        </property>
        <property name="updateTime" type="java.util.Date">
            <column name="UPDATE_TIME" precision="7" />
        </property>
        <property name="updateUserId" type="java.lang.String">
            <column name="UPDATE_USER_ID" length="32" />
        </property>
        <property name="projectType" type="java.lang.String">
            <column name="PROJECT_TYPE" length="200" />
        </property>
    </class>
</hibernate-mapping>
