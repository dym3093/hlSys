<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name = "org.hpin.base.usermanager.entity.Dept" table="UM_DEPT">
		<id name="id" type="java.lang.String" column="id">
	      <generator class="uuid"/>
	    </id>

		<property name="deptCode" type="java.lang.String">
			<column name="dept_Code" />
		</property>

		<property name="deptName" type="java.lang.String">
			<column name="dept_Name" />
		</property>
		
		<property name="deptAddress" type="java.lang.String">
			<column name="dept_Address" />
		</property>
		
		<property name="sortId" type="java.lang.Integer">
			<column name="sort_Id" />
		</property>
		
		<property name="deptLevel" type="java.lang.Integer">
			<column name="dept_Level" />
		</property>
		
		<property name="parentId" type="java.lang.String">
			<column name="parent_Id" />
		</property>
		
		<property name="createTime" type="java.sql.Date">
			<column name="CREATE_TIME" />
		</property>
		
		<property name="createUserId" type="java.lang.String">
			<column name="CREATE_USER_ID" />
		</property>
		
		<property name="createDeptId" type="java.lang.String">
			<column name="CREATE_DEPT_ID" />
		</property>
		
		<property name="isDeleted" type="java.lang.Integer">
			<column name="IS_DELETED" />
		</property>
		
		<property name="deleteTime" type="java.sql.Date">
			<column name="DELETE_TIME" />
		</property>
		
		<property name="deleteUserId" type="java.lang.String">
			<column name="DELETE_USER_ID" />
		</property>
		
		<property name="updateTime" type="java.sql.Date">
			<column name="UPDATE_TIME" />
		</property>
		
		<property name="updateUserId" type="java.lang.String">
			<column name="UPDATE_USER_ID" />
		</property>
		
		<property name="deptNameShort" type="java.lang.String">
			<column name="DEPT_NAME_SHORT" />
		</property>
	</class>
</hibernate-mapping>
