package org.hpin.warehouse.entity;

import org.hpin.common.core.orm.BaseEntity;

public class ErpApplicationDetail extends BaseEntity {

	/**
	 * 序列号
	 */
	private static final long serialVersionUID = 1540780853282739723L;
	
	/**主键Id*/
	private String id;
	/**申请单ID*/
	private String applicationId;
	/**产品套餐id*/
	private String proComboId; 
	private String productComboName; //显示所使用;
	
	/**产品ID*/
	private String productId;
	/**产品名称*/
	private String productName;
	/**规格*/
	private String standard;
	/**申请数量*/
	private Integer applicationCount;
	/**发货数量*/
	private Integer sendCount;
	/**状态：0已发货  1部分发货*/
	private Integer status;
	
	private String productTypeName;//类型名称
	
	public String getProductTypeName() {
		return productTypeName;
	}
	public void setProductTypeName(String productTypeName) {
		this.productTypeName = productTypeName;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getApplicationId() {
		return applicationId;
	}
	public void setApplicationId(String applicationId) {
		this.applicationId = applicationId;
	}
	public String getProductId() {
		return productId;
	}
	public void setProductId(String productId) {
		this.productId = productId;
	}
	public String getProductName() {
		return productName;
	}
	public void setProductName(String productName) {
		this.productName = productName;
	}
	public String getStandard() {
		return standard;
	}
	public void setStandard(String standard) {
		this.standard = standard;
	}
	public Integer getApplicationCount() {
		return applicationCount;
	}
	public void setApplicationCount(Integer applicationCount) {
		this.applicationCount = applicationCount;
	}
	
	public void setApplicationCountInt(String applicationCountInt) {
		this.applicationCount = Integer.parseInt(applicationCountInt);
	}
	
	public Integer getSendCount() {
		return sendCount;
	}
	public void setSendCount(Integer sendCount) {
		this.sendCount = sendCount;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public String getProComboId() {
		return proComboId;
	}
	public void setProComboId(String proComboId) {
		this.proComboId = proComboId;
	}
	public String getProductComboName() {
		return productComboName;
	}
	public void setProductComboName(String productComboName) {
		this.productComboName = productComboName;
	}
	
	
}
