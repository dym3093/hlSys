<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.hpin.base.customerrelationship.entity.ErpRelationShipCombo" table="ERP_RELATIONSHIPPRO_COMBO">
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="uuid" />
        </id>
        <property name="customerRelationShipProId" type="java.lang.String">
            <column name="CUSTOMER_RELATIONSHIP_PRO_ID"  />
        </property>
        
        <property name="comboId" type="java.lang.String">
            <column name="COMBO_ID"  />
        </property>
        
        <property name="isCreatePdf" type="java.lang.String">
            <column name="IS_CREATE_PDF"  />
        </property>
        
        <property name="comboShowName" type="java.lang.String">
            <column name="COMBO_SHOW_NAME"  />
        </property>
        
        <property name="isUsed" type="java.lang.String">
            <column name="IS_USED"  />
        </property>
        
        <property name="printType" type="java.lang.Integer">
            <column name="PRINT_TYPE"  />
        </property>
        
    </class>
</hibernate-mapping>
