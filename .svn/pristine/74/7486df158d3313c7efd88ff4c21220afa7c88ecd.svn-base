package cn.poi;

public class ExcelDelColUtil {
	private int start;
	private int end;
	private int col;
	public ExcelDelColUtil(){
		start=-1;
		end=-1;
		col=-1;
	}
	public int getStart() {
		return start;
	}
	public void setStart(int start) {
		this.start = start;
	}
	public int getEnd() {
		return end;
	}
	public void setEnd(int end) {
		this.end = end;
	}
	public int getCol() {
		return col;
	}
	public void setCol(int col) {
		this.col = col;
	}

}
