log4j.rootLogger=info, stdout , homelink
log4j.appender.homelink=org.apache.log4j.DailyRollingFileAppender
log4j.appender.homelink.File=${catalina.home}/logs/ymjy.log
log4j.appender.homelink.layout=org.apache.log4j.PatternLayout
log4j.appender.homelink.layout.ConversionPattern = %-d{yyyy-MM-dd HH:mm:ss} [ %t:%r ] - [ %p ] %m%n
log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.stdout.Target=System.out
log4j.appender.stdout.layout=org.apache.log4j.PatternLayout
log4j.appender.stdout.layout.ConversionPattern=%d{ABSOLUTE} %5p %c{1}\:%L - %m%n
log4j.logger.com.opensymphony.xwork2 =ERRdOR
log4j.logger.org.springframework.jdbc.core.JdbcTemplate=debug
#log4j.logger.org.hibernate.cache=debug

