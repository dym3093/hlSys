package org.hpin.physical.entity;

import org.hpin.common.core.orm.BaseEntity;

public class PhyReportItem extends BaseEntity {

	private String id;
	
	private String reportId;		//体检报告表ID
	
	private String geneDiseaseCode;	//基因疾病编码
	
	private String geneDiseaseName;	//基因疾病名字
	
	private String ymDiseaseCode;	//远盟疾病编码
	
	private String ymDiseaseName;	//远盟疾病名字
	
	private Integer geneRiskGrade;	//基因风险等级
	
	private String geneRiskRemark;	//基因风险描述
	
	private Integer ymRiskGrade;	//远盟风险等级
	
	private String ymRiskRemark;	//远盟风险描述
	
	private String phyItem;			//检测项列表
	
	private String geneCode;		//该用户的基因code
	
	private String showStatus;		//是否展示（默认选取三种疾病）

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getReportId() {
		return reportId;
	}

	public void setReportId(String reportId) {
		this.reportId = reportId;
	}

	public String getGeneDiseaseCode() {
		return geneDiseaseCode;
	}

	public void setGeneDiseaseCode(String geneDiseaseCode) {
		this.geneDiseaseCode = geneDiseaseCode;
	}

	public String getGeneDiseaseName() {
		return geneDiseaseName;
	}

	public void setGeneDiseaseName(String geneDiseaseName) {
		this.geneDiseaseName = geneDiseaseName;
	}

	public String getYmDiseaseCode() {
		return ymDiseaseCode;
	}

	public void setYmDiseaseCode(String ymDiseaseCode) {
		this.ymDiseaseCode = ymDiseaseCode;
	}

	public String getYmDiseaseName() {
		return ymDiseaseName;
	}

	public void setYmDiseaseName(String ymDiseaseName) {
		this.ymDiseaseName = ymDiseaseName;
	}

	public Integer getGeneRiskGrade() {
		return geneRiskGrade;
	}

	public void setGeneRiskGrade(Integer geneRiskGrade) {
		this.geneRiskGrade = geneRiskGrade;
	}

	public String getGeneRiskRemark() {
		return geneRiskRemark;
	}

	public void setGeneRiskRemark(String geneRiskRemark) {
		this.geneRiskRemark = geneRiskRemark;
	}

	public Integer getYmRiskGrade() {
		return ymRiskGrade;
	}

	public void setYmRiskGrade(Integer ymRiskGrade) {
		this.ymRiskGrade = ymRiskGrade;
	}

	public String getYmRiskRemark() {
		return ymRiskRemark;
	}

	public void setYmRiskRemark(String ymRiskRemark) {
		this.ymRiskRemark = ymRiskRemark;
	}

	public String getPhyItem() {
		return phyItem;
	}

	public void setPhyItem(String phyItem) {
		this.phyItem = phyItem;
	}

	public String getGeneCode() {
		return geneCode;
	}

	public void setGeneCode(String geneCode) {
		this.geneCode = geneCode;
	}

	public String getShowStatus() {
		return showStatus;
	}

	public void setShowStatus(String showStatus) {
		this.showStatus = showStatus;
	}

	@Override
	public String toString() {
		return "PhyReportItem [id=" + id + ", reportId=" + reportId
				+ ", geneDiseaseCode=" + geneDiseaseCode + ", geneDiseaseName="
				+ geneDiseaseName + ", ymDiseaseCode=" + ymDiseaseCode
				+ ", ymDiseaseName=" + ymDiseaseName + ", geneRiskGrade="
				+ geneRiskGrade + ", geneRiskRemark=" + geneRiskRemark
				+ ", ymRiskGrade=" + ymRiskGrade + ", ymRiskRemark="
				+ ymRiskRemark + ", phyItem=" + phyItem + ", geneCode="
				+ geneCode + ", showStatus=" + showStatus + "]";
	}
}
