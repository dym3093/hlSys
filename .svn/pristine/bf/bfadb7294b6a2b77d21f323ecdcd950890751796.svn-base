package org.hpin.statistics.briefness.entity;

import java.math.BigDecimal;
import java.util.Date;

import org.hpin.common.core.orm.BaseEntity;
/**
 * 基于公司报告汇总(财务)
 * @author tangxing
 * @date 2016-8-30下午3:22:09
 */
public class GeneReportSummaryFinance extends BaseEntity {

	private static final long serialVersionUID = 1L;

	private String id;						
	
	private String month;					//月份
	
	private String itemCode;				//项目编号
	
	private String itemHead;				//项目负责人
	
	private String itemName;				//项目名称
	
	private Integer haveReportNum;			//有报告人数
	
	private BigDecimal settlementAmountBX;	//保险公司结算金额
	
	private BigDecimal settlementAmountJY;	//基因公司结算金额
	
	private Date createDate;
	
	private String createUserName;
	
	private Date updateDate;
	
	private String updateUserName;

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getMonth() {
		return month;
	}

	public void setMonth(String month) {
		this.month = month;
	}

	public String getItemCode() {
		return itemCode;
	}

	public void setItemCode(String itemCode) {
		this.itemCode = itemCode;
	}

	public String getItemHead() {
		return itemHead;
	}

	public void setItemHead(String itemHead) {
		this.itemHead = itemHead;
	}

	public String getItemName() {
		return itemName;
	}

	public void setItemName(String itemName) {
		this.itemName = itemName;
	}

	public Integer getHaveReportNum() {
		return haveReportNum;
	}

	public void setHaveReportNum(Integer haveReportNum) {
		this.haveReportNum = haveReportNum;
	}

	public BigDecimal getSettlementAmountBX() {
		return settlementAmountBX;
	}

	public void setSettlementAmountBX(BigDecimal settlementAmountBX) {
		this.settlementAmountBX = settlementAmountBX;
	}

	public BigDecimal getSettlementAmountJY() {
		return settlementAmountJY;
	}

	public void setSettlementAmountJY(BigDecimal settlementAmountJY) {
		this.settlementAmountJY = settlementAmountJY;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}

	public String getCreateUserName() {
		return createUserName;
	}

	public void setCreateUserName(String createUserName) {
		this.createUserName = createUserName;
	}

	public Date getUpdateDate() {
		return updateDate;
	}

	public void setUpdateDate(Date updateDate) {
		this.updateDate = updateDate;
	}

	public String getUpdateUserName() {
		return updateUserName;
	}

	public void setUpdateUserName(String updateUserName) {
		this.updateUserName = updateUserName;
	}

	@Override
	public String toString() {
		return "GeneReportSummary [id=" + id + ", month=" + month
				+ ", itemCode=" + itemCode + ", itemHead=" + itemHead
				+ ", itemName=" + itemName + ", haveReportNum=" + haveReportNum
				+ ", settlementAmountBX=" + settlementAmountBX
				+ ", settlementAmountJY=" + settlementAmountJY
				+", createDate="
				+ createDate + ", createUserName=" + createUserName
				+ ", updateDate=" + updateDate + ", updateUserName="
				+ updateUserName + "]";
	}

}
