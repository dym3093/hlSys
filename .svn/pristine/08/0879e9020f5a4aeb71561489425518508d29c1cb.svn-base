<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.hpin.warehouse.entity.ErpProduct" table="ERP_PRODUCT">
        <id name="id" type="java.lang.String">
			<column name="ID" />
			<generator class="uuid" />
		</id>
        
        <property name="productType" type="java.lang.String">
            <column name="PRODUCT_TYPE"/>
        </property>
        <property name="productName" type="java.lang.String">
            <column name="PRODUCT_NAME"/>
        </property>
        <property name="standard" type="java.lang.String">
            <column name="STANDARD"/>
        </property>
        <property name="describe" type="java.lang.String">
            <column name="DESCRIBE"/>
        </property>
        <property name="isClose" type="java.lang.String">
            <column name="IS_CLOSE"/>
        </property>
        <property name="imagePath" type="java.lang.String">
            <column name="IMAGE_PATH"/>
        </property>
        
        <!-- 默认字段 -->
        <property name="createUserId" type="java.lang.String">
            <column name="CREATE_USER_ID"/>
        </property>
        <property name="createTime" type="java.util.Date">
            <column name="CREATE_TIME"/>
        </property>
        <property name="updateUserId" type="java.lang.String">
            <column name="UPDATE_USER_ID"/>
        </property>
        <property name="updateTime" type="java.util.Date">
            <column name="UPDATE_TIME" />
        </property>
        <property name="isDeleted" type="java.lang.Integer">
            <column name="IS_DELETED" precision="4" scale="0" />
        </property>
    </class>
</hibernate-mapping>
