<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.hpin.reportdetail.entity.ErpReportdetailManagement" table="ERP_REPORTDETAIL_MANAGEMENT">
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="uuid" />
        </id>
        
        <property name="name" type="java.lang.String">
            <column name="NAME"/>
        </property>
        
         <property name="code" type="java.lang.String">
            <column name="CODE"/>
        </property>
        
         <property name="gender" type="java.lang.String">
            <column name="gender"/>
        </property>
        
         <property name="excelName" type="java.lang.String">
            <column name="EXCELNAME"/>
        </property>
        
        <property name="createUser" type="java.lang.String">
            <column name="CREATEUSER"/>
        </property>
        
        <property name="createTime" type="java.util.Date">
            <column name="CREATETIME"/>
        </property>
        
       <property name="updateUser" type="java.lang.String">
            <column name="UPDATEUSER"/>
        </property>
        
        <property name="updateTime" type="java.util.Date">
            <column name="UPDATETIME" />
        </property>
        
        <property name="isDelete" type="java.lang.String">
            <column name="ISDELETE" />
        </property>
        
        <property name="state" type="java.lang.String">
            <column name="STATE" />
        </property>
        
        <property name="batchNo" type="java.lang.String">
            <column name="BATCHNO" />
        </property>
        
    </class>
</hibernate-mapping>
